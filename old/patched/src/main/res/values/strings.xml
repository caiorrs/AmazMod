    <string name="changelog_v_100">v1.0.0 - 11/02/2018</string>
    <string name="changelog_end">App modified by edotassi with valuable tips from John BNC</string>
    <string name="changelog_row_1"> - Notification management with last message from Whatsapp</string>
    <string name="changelog_row_2"> - Added UI sync Strava</string>
    <string name="changelog_row_3"> - Compatible with both US and Chinese versions</string>
    <string name="changelog_row_4"> - Added Italian official Facebook group link</string>
    <string name="changelog_row_5"> - Added check permissions on startup</string>
    <string name="changelog_row_6"> - Edit the photos in the Login page</string>
    <string name="changelog_row_7"> - Added notification app updates</string>
    <string name="changelog_row_8"> - Added donation link</string>
    <string name="changelog_row_9"> - Added email</string>

    <string name="changelog_v_200">v2.0.0 - 08/03/2018</string>
    <string name="changelog_row_10"> - Fix crash login on Oreo</string>
    <string name="changelog_row_11"> - Added individual notifications on the watch</string>
    <string name="changelog_row_12"> - Notifications on the watch are deleted (almost all)</string>
    <string name="changelog_row_13"> - Replaced Chinese maps with Google ones</string>
    <string name="changelog_row_14"> - Added mod\'s settings</string>
    <string name="changelog_row_15"> - Added the ability to enable / disable RTL support (requires full reboot of the app)</string>
    <string name="changelog_row_16"> - Disabled OTA service (very experimental)</string>
    <string name="changelog_row_17"> - Updated app based on version 2.2 Chinese</string>
    <string name="changelog_row_18"> - Started translations into Italian</string>

    <string name="changelog_v_210">v2.1.0 - 27/03/2018</string>
    <string name="changelog_row_19"> - Updated base app to Chinese version 2.3</string>
    <string name="changelog_row_20"> - Fix for "StatusFragment" crash</string>
    <string name="changelog_row_21"> - Added more options to Notifications settings</string>
    <string name="changelog_row_22"> - Fixed some translations</string>
    <string name="changelog_row_23"> - Added Telegram channel link for updates</string>
    <string name="changelog_row_24"> - Added link to the project on Github</string>
    <string name="changelog_row_25"> - Restored original icons of the track start / end markers</string>
    <string name="changelog_row_26"> - Fix for the "watch unpair" crash</string>
    <string name="changelog_row_27"> - Renamed app in AmazMod</string>

    <string name="changelog_v_300">v3.0.0 - 06/04/2018</string>
    <string name="changelog_row_28"> - Added battery chart</string>
    <string name="changelog_row_29"> - Fix notifications</string>

    <string name="changelog_v_400">v4.0.0 - 01/05/2018</string>
    <string name="changelog_row_30"> - Added the watchfaces custom store (https://sawfb.fabiobarbon.click/)</string>
    <string name="changelog_row_31"> - Bug fixing</string>
    <string name="changelog_row_32"> - Added translations into Brazilian Portuguese</string>
    <string name="changelog_row_33"> - Added partial translations in Spanish</string>

    <string name="changelog_v_415">v4.1.5 - 08/05/2018</string>
    <string name="changelog_row_34"> - Fixed crash when update is available</string>
    <string name="changelog_row_35"> - Fixed bug in notifications</string>
    <string name="changelog_row_36"> - Added Greek translations</string>
    <string name="changelog_row_37"> - Updated Italian translations</string>

    <string name="changelog_v_416">v4.1.6 - 08/05/2018</string>
    <string name="changelog_row_38"> - Now the google map also works with other types of visualization</string>
    <string name="changelog_row_39"> - Fixed some Italian strings</string>

    <string name="changelog_v_420">v4.2.0 - 15/05/2018</string>
    <string name="changelog_row_40"> - Added another store among those available (johnbnc)</string>
    <string name="changelog_row_41"> - Support Android 8 Adaptive Icon</string>
    <string name="changelog_row_42"> - Updated Italian translations</string>
    <string name="changelog_row_43"> - Initial support for AmazModService</string>
    <string name="changelog_row_44"> - Added Polish translations</string>

